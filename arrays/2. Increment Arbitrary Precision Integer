// TC O(N), SC O(1)

vector<int> increment(vector<int> A) {
    reverse(A.begin(), A.end());
    int carry = 1;
    
    for(int i = 0; i < A.size(); i++) {
        int tmp = A[i];
        tmp += carry;
        
        if(tmp >= 10) {
            A[i] = tmp % 10;
            carry = 1;
        }
        else {
            A[i] = tmp;
            carry = 0;
        }
    }
    
    if(carry) A.push_back(carry);
    reverse(A.begin(), A.end());
    return A;
}
